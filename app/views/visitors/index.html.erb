               <div class="table-responsive">
        <table class="table table-striped">
          <thead>
            <tr>
              <th>#ID</th>
              <th>Estimated value</th>
              <th>Margin</th>
              <th>Profit</th>
              <th>Composition</th>
            </tr>
          </thead>
          <tbody>
            <% @asteroids.each do |asteroid| %>
              <% p asteroid %>
              <tr>
                <td><%= asteroid["full_name"] %></td>
                <td><%= Round(asteroid["price"]) %></td>
                <td><%= Round(100*asteroid["profit"]/asteroid["price"]) %></td>
                <td><%= Round(asteroid["profit"]) %></td>
                <td><%=  %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
